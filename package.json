{
    "name": "npm-dependency-inspector",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview --port 4173",
    "lint": "eslint , --ext .ts,.tsx",
