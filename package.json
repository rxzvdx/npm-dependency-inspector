    "prepare": "husky",
    "precommit": "npm audit --audit-level=high && npm run lint"
    },
    "dependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.1",
        "eslint": "^9.9.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.9",
        "husky": "^9.0.11",
        "typescript": "^5.5.3",
        "vite": "^5.4.3"

    }   
}